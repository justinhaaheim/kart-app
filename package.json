{
  "name": "kart-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "clean:build": "rm -rf build/",
    "clean:node-modules": "rm -rf node_modules/",
    "dev": "vite",
    "lint:fix": "bun run lint --fix",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier-check": "prettier --check .",
    "prettier": "prettier --write .",
    "preview": "vite preview",
    "signal:serial": "bun run ts-check && bun run lint && bun run prettier-check",
    "signal": "concurrently --names \"TS,LINT,PRETTIER\" -c \"bgBlack.bold,bgRed.bold,bgCyan.bold\" \"bun run ts-check\" \"bun run lint\" \"bun run prettier-check\"",
    "ts-check": "tsc --noEmit",
    "ts-check-masked": "bun run ts-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "8.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "lint-staged": {
    "**/*": "prettier --check --ignore-unknown",
    "*.(js|jsx|cjs|ts|tsx)": [
      "eslint --cache --max-warnings=0"
    ]
  }
}
